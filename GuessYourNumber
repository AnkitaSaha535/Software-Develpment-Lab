import random

target = random.randint(1, 50)
attempts = 0

print("I am thinking of a number between 1 and 50.")

while True:
    guess = int(input("Enter your guess: "))
    attempts += 1
    
    if guess < target:
        print("Higher!")
    elif guess > target:
        print("Lower!")
    else:
        print(f"Correct! It took you {attempts} attempts.")
        break
